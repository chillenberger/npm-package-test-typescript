{
    "name": "@my-domain/my-repo",
    "version": "0.2.1",
    "description": "A simple test package",
    "typings": "types/types.d.ts",
    "keywords": [
        "components"
    ],
    "main": "dist/index.js",
    "files": [
        "dist/",
        "types",
        "README.md",
        "package.json"
    ],
    "scripts": {
        "preinstall": "npm run co:login",
        "co:login": "aws codeartifact login --tool npm --repository my-repo --domain my-domain",
        "test": "echo \"Error: no test specified\" && exit 1",
        "build": "rm -rf dist && ./node_modules/.bin/babel --extensions .ts src --out-dir ./dist"
    },
    "devDependencies": {
        "@babel/cli": "^7.18.10",
        "@babel/core": "^7.18.10",
        "@babel/preset-env": "^7.18.10",
        "@babel/preset-typescript": "^7.18.6"
    }
}
